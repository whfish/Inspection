<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.inspection">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_title2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_title2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.DevStateActivity">
        </activity>
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.MainActivity"
            android:launchMode="singleTask"></activity>
        <activity android:name=".ui.StatusActivity" />
        <activity android:name=".SystemDetailsActivity"></activity>
        <activity android:name=".ui.EquipmentDetailsActivity"></activity>
        <activity android:name=".ui.Fragment.barchart.XgPwd"></activity>
        <activity android:name=".ui.NetWork.NetWorkActivity"></activity>
        <activity android:name=".SystemModActivity"></activity>
        <activity android:name=".SystemAddActivity"></activity>
        <activity android:name=".SystemListActivity"></activity>


    </application>

</manifest>